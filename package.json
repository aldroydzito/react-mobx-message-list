    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "autoprefixer": "^9.4.3",
    "concurrently": "^4.1.0",
    "node-sass": "^4.11.0",
    "postcss-cli": "^6.1.0",
    "tailwindcss": "^0.7.3"
  }
