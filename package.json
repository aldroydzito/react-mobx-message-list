{
  "name": "react-mobx",
  "version": "0.1.0",
  "private": true,
  "homepage" : "http://aldroydzito.github.io/react-mobx-message-list",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.6.3",
    "mobx": "^5.8.0",
    "mobx-react": "^5.4.3",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-scripts": "2.1.2"
  },
  "scripts": {
    "dev": "concurrently \"npm run watch:css\" \"npm start\"",
    "watch:css": "postcss src/styles/index.css -o src/index.css -w",
